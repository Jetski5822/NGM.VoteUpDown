@model NGM.VoteUpDown.Settings.VoteUpDownTypePartSettings
<fieldset>
    <div>
        @Html.EditorFor(m => m.ShowVoter)
        <label class="forcheckbox" for="@Html.FieldIdFor(m => m.ShowVoter)">@T("Enable up/down rating")</label>
    </div>
    <div>
        @Html.EditorFor(m => m.AllowAnonymousRatings)
        <label class="forcheckbox" for="@Html.FieldIdFor(m => m.AllowAnonymousRatings)">@T("Allow anonymous ratings")</label>
    </div>
</fieldset>